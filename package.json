{
	"name": "asdfui",
	"version": "0.1.2",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint '{src,test}/**/*.ts' --cache",
		"lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint '{src,test}/**/*.ts' --cache --fix",
		"prerelease": "npm run build",
		"release": "bumpp && npm release"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^1.0.0",
		"execa": "^8.0.1",
		"ink": "^4.4.1",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^3.0.0",
		"ink-link": "^3.0.0",
		"meow": "^11.0.0",
		"react": "^18.2.0",
		"zustand": "^4.4.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^5.0.0",
		"@types/react": "^18.2.33",
		"bumpp": "^9.2.0",
		"chalk": "^5.3.0",
		"eslint": "^8.52.0",
		"prettier": "^3.0.3",
		"react-devtools-core": "^4.28.5",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
}
