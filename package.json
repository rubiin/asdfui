{
	"name": "asdfui",
	"version": "1.0.5",
	"bin": "dist/index.js",
	"type": "module",
	"description": "Experimental TUI for asdf vm",
	"author": {
		"email": "roobin.bhandari@gmail.com",
		"name": "Rubin Bhandari",
		"url": "https://rubiin.vercel.app"
	},
	"license": "MIT",
	"homepage": "https://github.com/rubiin/asdfui#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/rubiin/asdfui.git"
	},
	"bugs": "https://github.com/rubiin/asdfui/issues",
	"keywords": [
		"cli",
		"asdf",
		"node",
		"nvm",
		"n",
		"asdfui",
		"node.js",
		"volta"
	],
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "rimraf dist && tsup src/index.ts --format esm,cjs --clean --dts",
		"dev": "tsup --watch src/index.ts",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint '{src,test}/**/*.ts' --cache",
		"lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint '{src,test}/**/*.ts' --cache --fix",
		"prerelease": "npm run build",
		"release": "bumpp && npm publish",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^1.0.0",
		"execa": "^8.0.1",
		"ink": "^4.4.1",
		"ink-select-input": "^5.0.0",
		"is-online": "^10.0.0",
		"react": "^18.2.0",
		"tsup": "^7.3.0",
		"zustand": "^4.4.6"
	},
	"devDependencies": {
		"@rubiin/eslint-config": "^1.9.15",
		"@sindresorhus/tsconfig": "^5.0.0",
		"@types/node": "^20.9.4",
		"@types/react": "^18.2.38",
		"bumpp": "^9.2.0",
		"chalk": "^5.3.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.54.0",
		"prettier": "^3.1.0",
		"react-devtools-core": "^4.28.5",
		"rimraf": "^5.0.5",
		"typescript": "^5.3.2"
	}
}
