{
	"name": "asdfui",
	"version": "0.1.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava",
		"format": "prettier --write --cache \"src/**/*.{ts,tsx}\""
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^1.0.0",
		"execa": "^8.0.1",
		"ink": "^4.4.1",
		"meow": "^11.0.0",
		"react": "^18.2.0",
		"zustand": "^4.4.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.2.33",
		"ava": "^5.3.1",
		"chalk": "^5.3.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^3.0.3",
		"react-devtools-core": "^4.28.5",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	}
}
